<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="root" hgap="5.0" prefWidth="300.0" vgap="5.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.transportViewModel.AutoViewModel">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <children>
        <Label text="Width" textFill="WHITE" />
        <TextField fx:id="width" style="-fx-background-radius: 0;" GridPane.columnIndex="1" />
        <Label text="Height" textFill="WHITE" GridPane.rowIndex="1" />
        <TextField fx:id="height" style="-fx-background-radius: 0;" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Label text="Depth" textFill="WHITE" GridPane.rowIndex="2" />
        <TextField fx:id="depth" style="-fx-background-radius: 0;" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <Label text="MaxSpeed" textFill="WHITE" GridPane.rowIndex="3" />
        <TextField fx:id="maxSpeed" style="-fx-background-radius: 0;" GridPane.columnIndex="1" GridPane.rowIndex="3" />
        <Label text="Team count" textFill="WHITE" GridPane.rowIndex="4" />
        <TextField fx:id="teamCount" style="-fx-background-radius: 0;" GridPane.columnIndex="1" GridPane.rowIndex="4" />
        <Label text="Passages count" textFill="WHITE" GridPane.rowIndex="5" />
        <TextField fx:id="passagesCount" style="-fx-background-radius: 0;" GridPane.columnIndex="1" GridPane.rowIndex="5" />
        <Label text="Min weight" textFill="WHITE" GridPane.rowIndex="6" />
        <TextField fx:id="minWeight" style="-fx-background-radius: 0;" GridPane.columnIndex="1" GridPane.rowIndex="6" />
        <Label text="Max weight" textFill="WHITE" GridPane.rowIndex="7" />
        <TextField fx:id="maxWeight" style="-fx-background-radius: 0;" GridPane.columnIndex="1" GridPane.rowIndex="7" />
        <Label text="Acceleration time to 100" textFill="WHITE" GridPane.rowIndex="8" />
        <TextField fx:id="accelerationTimeTo100" style="-fx-background-radius: 0;" GridPane.columnIndex="1" GridPane.rowIndex="8" />
        <Label text="Mark" textFill="WHITE" GridPane.rowIndex="9" />
        <TextField fx:id="mark" style="-fx-background-radius: 0;" GridPane.columnIndex="1" GridPane.rowIndex="9" />
        <Label text="Transmission type" textFill="WHITE" GridPane.rowIndex="10" />
        <TextField fx:id="transmissionType" style="-fx-background-radius: 0;" GridPane.columnIndex="1" GridPane.rowIndex="10" />
    </children>
</GridPane>
